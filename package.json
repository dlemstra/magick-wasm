{
    "name": "@imagemagick/magick-wasm",
    "version": "0.0.15",
    "description": "The WASM library for ImageMagick",
    "keywords": [
        "imagemagick",
        "magick",
        "wasm",
        "image",
        "convert",
        "resize"
    ],
    "homepage": "https://github.com/dlemstra/magick-wasm",
    "bugs": {
        "url": "https://github.com/dlemstra/magick-wasm/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dlemstra/magick-wasm.git"
    },
    "license": "Apache-2.0",
    "author": "Dirk Lemstra",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.umd.js",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.umd.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "NOTICE"
    ],
    "scripts": {
        "build": "vite build && dts-bundle-generator src/index.ts -o dist/index.d.ts --external-inlines @dlemstra/magick-native",
        "test": "node ./node_modules/jest/bin/jest.js --bail --runInBand --detectOpenHandles --forceExit --logHeapUsage",
        "test14": "node --experimental-wasm-threads --wasm-atomics-on-non-shared-memory ./node_modules/jest/bin/jest.js --bail --runInBand --detectOpenHandles --forceExit --logHeapUsage",
        "lint": "node ./node_modules/eslint/bin/eslint.js src tests --ext .ts",
        "demo": "tsc && cd demo && tsc && node --experimental-wasm-threads --wasm-atomics-on-non-shared-memory demo.js",
        "prepare": "copyfiles -f ./node_modules/@dlemstra/magick-native/NOTICE ."
    },
    "devDependencies": {
        "@dlemstra/magick-native": "0.202209.192139",
        "@types/jest": "^29.0.3",
        "@types/jsdom": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "ansi-regex": ">=6.0.1",
        "copyfiles": "^2.4.1",
        "dts-bundle-generator": "^6.13.0",
        "eslint": "^8.23.1",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "set-value": "^4.1.0",
        "ts-jest": "^29.0.1",
        "typescript": "^4.8.3",
        "vite": "^3.1.3"
    }
}
